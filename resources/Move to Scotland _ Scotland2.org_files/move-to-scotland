			const headerVideo = document.querySelector('.header-gateway__poster-video-container video');

			const mediaQChange = (e) => {
				if (headerVideo) {
				if (e.matches) {
					headerVideo.removeAttribute('autoplay');
					headerVideo.setAttribute('preload', 'none');			
